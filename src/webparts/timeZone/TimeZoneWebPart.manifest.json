      "description": {
        "default": "TimeZone description"
      },
      "officeFabricIconFontName": "Page",
      "properties": {
        "Title": "World Clock",
        "checkboxIsraelTelAviv": true,
        "checkboxAmericaNewYork": true,
        "checkboxAmericaChicago": true,
        "checkboxAmericaDenver": true
